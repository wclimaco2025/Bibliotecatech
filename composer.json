{
  "name": "climaco/biblioteca",
  "description": "Sistema de gestión de biblioteca con préstamos de libros",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Climaco",
      "email": "info@climaco.com"
    }
